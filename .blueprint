models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable', password: string, remember_token: 'string:100 nullable' }
    Conference: { name: string, description: string, start_date: datetime, end_date: datetime, status: string, timezone: string, region: string, relationships: { belongsTo: Venue } }
    Venue: { name: string, city: string, country: string, postal_code: string, region: string, relationships: { belongsToMany: Conference } }
    Speaker: { name: string, bio: text, twitter_handle: string, email: string, relationships: { belongsToMany: 'Conference, Talk' } }
    Category: { name: string, description: text, relationships: { hasMany: Talk } }
    Talk: { title: string, description: text, conference_id: 'unsigned integer', speaker_id: 'unsigned integer', category_id: 'unsigned integer', relationships: { belongsTo: 'Speaker, Conference, Category' } }
